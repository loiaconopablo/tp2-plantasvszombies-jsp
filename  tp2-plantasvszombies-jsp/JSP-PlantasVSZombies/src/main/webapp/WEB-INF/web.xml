<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>searchServlet</servlet-name>
		<servlet-class>controllers.SearchServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>IrAlOtroJardinServlet</servlet-name>
		<servlet-class>controllers.IrAlOtroJardinServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>MejorasServlet</servlet-name>
		<servlet-class>controllers.MejorasServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>searchServlet</servlet-name>
		<url-pattern>/search</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>IrAlOtroJardinServlet</servlet-name>
		<url-pattern>/irAlOtroJardin</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MejorasServlet</servlet-name>
		<url-pattern>/mejorarPlanta</url-pattern>
	</servlet-mapping>
	

	<taglib>
		<taglib-uri>http://java.sun.com/jstl/core</taglib-uri>
		<taglib-location>c.tld</taglib-location>
	</taglib>

</web-app>
